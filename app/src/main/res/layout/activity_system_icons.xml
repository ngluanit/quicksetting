                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvHotspot"
                    android:layout_height="wrap_content"
                    android:text="Hotspot"
                    android:layout_toRightOf="@id/imgHotspot"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnHotspot"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_switch_off" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlLocation"
                android:layout_below="@+id/rlHotspot"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_20sdp">
                <ImageView
                    android:id="@+id/imgLocation"
                    android:layout_width="@dimen/_24sdp"
                    android:layout_height="@dimen/_24sdp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_location_line"
                    android:tint="#737373"/>
                <TextView
                    android:layout_toEndOf="@id/imgLocation"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvLocation"
                    android:layout_height="wrap_content"
                    android:text="Location"
                    android:layout_toRightOf="@id/imgLocation"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnLocation"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_switch_off" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlMobiedata"
                android:layout_below="@+id/rlLocation"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_20sdp">
                <ImageView
                    android:id="@+id/imgMobiedata"
                    android:layout_width="@dimen/_24sdp"
                    android:layout_height="@dimen/_24sdp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_data_moblie"
                    android:tint="#737373"/>
                <TextView
                    android:layout_toEndOf="@id/imgMobiedata"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvMobiedata"
                    android:layout_height="wrap_content"
                    android:text="Mobile data"
                    android:layout_toRightOf="@id/imgMobiedata"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnMobiedata"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_switch_off" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlNFC"
                android:layout_below="@+id/rlMobiedata"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_20sdp">
                <ImageView
                    android:id="@+id/imgNFC"
                    android:layout_width="@dimen/_24sdp"
                    android:layout_height="@dimen/_24sdp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_nfc_line"
                    android:tint="#737373"/>
                <TextView
                    android:layout_toEndOf="@id/imgNFC"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvNFC"
                    android:layout_height="wrap_content"
                    android:text="NFC"
                    android:layout_toRightOf="@id/imgNFC"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnNFC"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_switch_off" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlSound"
                android:layout_below="@+id/rlNFC"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_20sdp">
                <ImageView
                    android:id="@+id/imgSound"
                    android:layout_width="@dimen/_24sdp"
                    android:layout_height="@dimen/_24sdp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_sound"
                    android:tint="#737373"/>
                <TextView
                    android:layout_toEndOf="@id/imgSound"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvSound"
                    android:layout_height="wrap_content"
                    android:text="Sound"
                    android:layout_toRightOf="@id/imgSound"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnSound"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_switch_off" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlSync"
                android:layout_below="@+id/rlSound"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_20sdp">
                <ImageView
                    android:id="@+id/imgSync"
                    android:layout_width="@dimen/_24sdp"
                    android:layout_height="@dimen/_24sdp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_sync_line"
                    android:tint="#737373"/>
                <TextView
                    android:layout_toEndOf="@id/imgSync"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvSync"
                    android:layout_height="wrap_content"
                    android:text="Sync"
                    android:layout_toRightOf="@id/imgSync"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnSync"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_switch_off" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlVPN"
                android:layout_below="@+id/rlSync"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_20sdp">
                <ImageView
                    android:id="@+id/imgVPN"
                    android:layout_width="@dimen/_24sdp"
                    android:layout_height="@dimen/_24sdp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_key"
                    android:tint="#737373"/>
                <TextView
                    android:layout_toEndOf="@id/imgVPN"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvVPN"
                    android:layout_height="wrap_content"
                    android:text="VPN"
                    android:layout_toRightOf="@id/imgVPN"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnVPN"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:src="@drawable/ic_switch_off" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlWifi"
                android:layout_below="@+id/rlVPN"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:paddingHorizontal="@dimen/_20sdp">
                <ImageView
                    android:id="@+id/imgWifi"
                    android:layout_width="@dimen/_24sdp"
                    android:layout_height="@dimen/_24sdp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_wifi"
                    android:tint="#737373"/>
                <TextView
                    android:layout_toEndOf="@id/imgWifi"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_width="wrap_content"
                    android:layout_centerVertical="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_14sdp"
                    android:id="@+id/tvWifi"
                    android:layout_height="wrap_content"
                    android:text="Wi-Fi"
                    android:layout_toRightOf="@id/imgWifi"
                    android:layout_marginLeft="@dimen/_20sdp" />
                <ImageView
                    android:id="@+id/imgTurnWifi"
                    android:layout_width="@dimen/_40sdp"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
